{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "strict": true,
    "skipLibCheck": true,
    "types": [
      "node"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "outDir": "./dist",
    
    "baseUrl": ".",
    "paths": {
      "@backend/*": ["src/*"]
    }
  },
  "exclude": ["node_modules"]
}
